{
  "name": "patronage-realtor",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "Patronage Realtor - Monorepo",
  "workspaces": [
    "packages/client",
    "packages/server",
    "packages/shared"
  ],
  "scripts": {
    "dev": "npm --workspace=packages/server run dev",
    "dev:client": "npm --workspace=packages/client run dev",
    "dev:server": "npm --workspace=packages/server run dev:server",
    "build": "npm --workspace=packages/server run build",
    "build:client": "npm --workspace=packages/client run build",
    "start": "npm --workspace=packages/server run start",
    "check": "npm run check --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "npm-run-all": "^4.1.5"
  }
}
